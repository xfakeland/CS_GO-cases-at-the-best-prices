function trans(text)
{
	if(cur_lang == "eng")
	{
		return text;
	}
	else
	{
		if(translations[text][id_lang-1] != undefined)
			return translations[text][id_lang-1];
		else
			return text;//return '!!!ERROR!!!';
	}	
}